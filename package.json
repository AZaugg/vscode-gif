{
  "name": "giphy",
  "displayName": "giphy",
  "description": "Easily grab a gif from within VS Code to add your to docs, presentations, etc.",
  "version": "0.0.1",
  "publisher": "jstewart",
  "engines": {
    "vscode": "^1.11.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:giphy.trending",
    "onCommand:giphy.agree",
    "onCommand:giphy.awww",
    "onCommand:giphy.dance",
    "onCommand:giphy.deal",
    "onCommand:giphy.eww",
    "onCommand:giphy.facepalm",
    "onCommand:giphy.fist",
    "onCommand:giphy.gl",
    "onCommand:giphy.bye",
    "onCommand:giphy.hug",
    "onCommand:giphy.idk",
    "onCommand:giphy.no",
    "onCommand:giphy.omg",
    "onCommand:giphy.mic",
    "onCommand:giphy.please",
    "onCommand:giphy.oops",
    "onCommand:giphy.seriously",
    "onCommand:giphy.shocked",
    "onCommand:giphy.shrug",
    "onCommand:giphy.sorry",
    "onCommand:giphy.thanks",
    "onCommand:giphy.yes"
  ],
  "main": "./out/src/main",
  "contributes": {
    "configuration": {
        "type": "object",
        "title": "GIPHY configuration",
        "properties": {
            "giphy.rating": {
                "type": "string",
                "default": "pg-13",
                "description": "Specify the rating you would like to use for GIPHY results."
            }
        }
    },
    "commands": [
      {
        "command": "giphy.trending",
        "title": "GIPHY Trending"
      },
      {
        "command": "giphy.agree",
        "title": "GIPHY Agree"
      },
      {
        "command": "giphy.aww",
        "title": "GIPHY Aww"
      },
      {
        "command": "giphy.dance",
        "title": "GIPHY Dance"
      },
      {
        "command": "giphy.deal",
        "title": "GIPHY Deal"
      },
      {
        "command": "giphy.eww",
        "title": "GIPHY Eww"
      },
      {
        "command": "giphy.facepalm",
        "title": "GIPHY Facepalm"
      },
      {
        "command": "giphy.fist",
        "title": "GIPHY Fist"
      },
      {
        "command": "giphy.gl",
        "title": "GIPHY Good Luck"
      },
      {
        "command": "giphy.bye",
        "title": "GIPHY Bye"
      },
      {
        "command": "giphy.hug",
        "title": "GIPHY Hug"
      },
      {
        "command": "giphy.idk",
        "title": "GIPHY Idk"
      },
      {
        "command": "giphy.no",
        "title": "GIPHY No"
      },
      {
        "command": "giphy.omg",
        "title": "GIPHY Omg"
      },
      {
        "command": "giphy.mic",
        "title": "GIPHY Mic Drop"
      },
      {
        "command": "giphy.please",
        "title": "GIPHY Please"
      },
      {
        "command": "giphy.oops",
        "title": "GIPHY Oops"
      },
      {
        "command": "giphy.seriously",
        "title": "GIPHY Seriously"
      },
      {
        "command": "giphy.shocked",
        "title": "GIPHY Shocked"
      },
      {
        "command": "giphy.shrug",
        "title": "GIPHY Shrug"
      },
      {
        "command": "giphy.sorry",
        "title": "GIPHY Sorry"
      },
      {
        "command": "giphy.thanks",
        "title": "GIPHY Thanks"
      },
      {
        "command": "giphy.yes",
        "title": "GIPHY Yes"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "handlebars": "^4.0.6",
    "mkdirp": "^0.5.1",
    "request": "^2.81.0",
    "request-promise": "^4.2.0",
    "rimraf": "^2.6.1",
    "uuid": "^3.0.1"
  }
}
